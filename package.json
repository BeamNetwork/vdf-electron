{
  "name": "vdf-electron",
  "version": "1.0.6",
  "description": "Electron packaging of vdf-solver",
  "main": "app/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && electron-rebuild",
    "lint": "eslint '**/**.js'",
    "format": "eslint --fix '**/**.js'",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BeamNetwork/vdf-electron.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BeamNetwork/vdf-electron/issues"
  },
  "homepage": "https://github.com/BeamNetwork/vdf-electron#readme",
  "dependencies": {
    "@sentry/electron": "^0.17.1",
    "@sentry/node": "^5.5.0",
    "body-parser": "^1.19.0",
    "deep-equal": "^1.0.1",
    "deepcopy": "^2.0.0",
    "electron-log": "^3.0.6",
    "electron-updater": "^4.1.2",
    "emittery": "^0.4.1",
    "express": "^4.17.1",
    "vdf-solver": "github:BeamNetwork/vdf-solver#master"
  },
  "devDependencies": {
    "@sentry/cli": "^1.46.0",
    "babel-eslint": "^10.0.2",
    "electron": "^5.0.6",
    "electron-builder": "^21.0.15",
    "electron-download": "^4.1.1",
    "electron-rebuild": "^1.8.5",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0"
  },
  "build": {
    "appId": "io.beam.vdf",
    "mac": {
      "category": "public.app-category.finance"
    }
  }
}
