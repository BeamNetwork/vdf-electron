  "version": "1.0.4",
  "description": "Electron packaging of vdf-solver",
  "main": "app/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && electron-rebuild",
    "lint": "eslint '**/**.js'",
    "format": "eslint --fix '**/**.js'",
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
