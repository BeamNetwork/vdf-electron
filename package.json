  "repository": {
    "type": "git",
    "url": "git+https://github.com/BeamNetwork/vdf-electron.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BeamNetwork/vdf-electron/issues"
  },
  "homepage": "https://github.com/BeamNetwork/vdf-electron#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "deep-equal": "^1.0.1",
    "deepcopy": "^2.0.0",
    "electron-log": "^3.0.6",
    "electron-updater": "^4.0.14",
    "emittery": "^0.4.1",
    "express": "^4.17.1",
    "vdf-solver": "github:BeamNetwork/vdf-solver#master"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "electron": "^5.0.6",
    "electron-builder": "^20.44.4",
    "electron-rebuild": "^1.8.5",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0"
